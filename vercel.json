{
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        { "type": "host", "value": "talkangels.com" },
        { "type": "missing", "value": "profile" },
        { "type": "missing", "value": "refer" },
        { "type": "missing", "value": "open" },
        { "type": "missing", "value": ".well-known" }
      ],
      "destination": "https://www.talkangels.com/:path*",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/.well-known/assetlinks.json",
      "headers": [
        { "key": "Content-Type", "value": "application/json" },
        { "key": "Cache-Control", "value": "public, max-age=3600" }
      ]
    }
  ]
}
